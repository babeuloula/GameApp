{
    "application": {
        "single_instance_guid": "",
        "dpi_aware": true
    },
    "debugging": {
        "show_console": false,
        "subprocess_show_console": false,
        "log_level": "DEBUG4",
        "log_file": "console.log"
    },
    "main_window": {
        "title": "Param√®tre de GameApp",
        "icon": "app/img/params.ico",
